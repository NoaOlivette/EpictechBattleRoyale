config_version=4

[application]

config/name="Epic Tech - Client"
run/main_scene="res://Scene/Lobby.tscn"
config/icon="res://Asset/icon.png"

[autoload]

Server="*res://Script/Server.gd"
Persist="*res://Scene/Persist.tscn"

[debug]

multirun/number_of_windows=3
multirun/window_distance=1700
multirun/add_custom_args=true
multirun/first_window_args="listen"
multirun/other_window_args="join"

[display]

window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/multirun/plugin.cfg" )

